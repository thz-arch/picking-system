import jsPDF from 'jspdf';

/**
 * Generate PDF from CTRC data
 * @param {Object} data - The CTRC data
 * @returns {jsPDF} - The generated PDF document
 */
export const generatePDF = (data) => {
  const doc = new jsPDF({
    orientation: 'landscape',
    unit: 'mm',
    format: 'a4'
  });

  // Cores
  const grayBg = [200, 200, 200];
  const borderColor = [100, 100, 100];
  const textColor = [0, 0, 0];

  let yPos = 10;

  // ===== CABEÇALHO =====
  // Logo Binho (canto esquerdo)
  const logoBase64 = 'data:image/png;base64,';
  
  try {
    doc.addImage(logoBase64, 'PNG', 10, yPos, 30, 12);
  } catch (e) {
    // Se falhar ao carregar logo, continuar sem ele
  }
  
  // Caixa cinza com título (ajustado para dar espaço ao logo)
  doc.setFillColor(...grayBg);
  doc.setDrawColor(...borderColor);
  doc.rect(10, yPos, 277, 15, 'FD');
  
  doc.setFontSize(20);
  doc.setTextColor(...textColor);
  doc.setFont('helvetica', 'bold');
  doc.text('CHECK LIST FULL SERVICE', 148, yPos + 10, { align: 'center' });
  
  yPos += 15;

  // Data de emissão (canto direito)
  doc.setFontSize(9);
  doc.setFont('helvetica', 'normal');
  const now = new Date();
  const emissao = `Emissão: ${now.getDate()}/${now.getMonth() + 1}/${now.getFullYear().toString().slice(-2)} ${now.getHours()}:${String(now.getMinutes()).padStart(2, '0')}`;
  doc.text(emissao, 195, yPos + 5, { align: 'right' });

  yPos += 5;

  // ===== SEÇÃO DE INFORMAÇÕES =====
  doc.setDrawColor(...borderColor);
  doc.rect(10, yPos, 190, 28, 'D');
  
  yPos += 7;
  doc.setFontSize(10);
  
  // Linha 1: Remetente e Separador
  doc.setFont('helvetica', 'bold');
  doc.text('Remetente:', 12, yPos);
  doc.setFont('helvetica', 'normal');
  doc.text(data.remetente || 'N/A', 35, yPos);
  
  doc.setFont('helvetica', 'bold');
  doc.text('Separador:', 130, yPos);
  doc.line(155, yPos, 198, yPos);
  
  yPos += 7;

  // Linha 2: Cliente e Paletes
  doc.setFont('helvetica', 'bold');
  doc.text('Cliente:', 12, yPos);
  doc.setFont('helvetica', 'normal');
  doc.text(data.destinatario || 'N/A', 35, yPos);
  
  doc.setFont('helvetica', 'bold');
  doc.text('Paletes:', 130, yPos);
  doc.line(155, yPos, 198, yPos);
  
  yPos += 7;

  // Linha 3: CTRC, Nota Fiscal, Volumes, Data e Assinatura
  doc.setFont('helvetica', 'bold');
  doc.text('CTRC:', 12, yPos);
  doc.setFont('helvetica', 'normal');
  doc.text(data.ctrc || 'N/A', 28, yPos);
  
  doc.setFont('helvetica', 'bold');
  doc.text('NF:', 55, yPos);
  doc.setFont('helvetica', 'normal');
  doc.text(data.notaFiscal || 'N/A', 65, yPos);
  
  doc.setFont('helvetica', 'bold');
  doc.text('VOL:', 88, yPos);
  doc.setFont('helvetica', 'normal');
  doc.text(data.qvol || 'N/A', 100, yPos);
  
  doc.setFont('helvetica', 'bold');
  doc.text('Data:', 115, yPos);
  doc.line(128, yPos, 155, yPos);
  
  doc.setFont('helvetica', 'bold');
  doc.text('Assintura:', 160, yPos);
  doc.line(180, yPos, 198, yPos);
  
  yPos += 12;

  // ===== SEÇÃO DE OBSERVAÇÕES =====
  if (data.observacao) {
    const alturaBox = 30; // Altura dinâmica baseada nas observações
    doc.setDrawColor(...borderColor);
    doc.rect(10, yPos, 190, alturaBox, 'D');
    
    yPos += 5;
    doc.setFontSize(9);
    doc.setFont('helvetica', 'bold');
    doc.text('OBSERVAÇÕES:', 12, yPos);
    yPos += 5;
    
    doc.setFontSize(8);
    doc.setFont('helvetica', 'normal');
    
    const obs = data.observacao;
    
    // Coluna esquerda
    let leftY = yPos;
    if (obs['Perfil de Recebimento']) {
      doc.text(`Perfil de Recebimento: ${obs['Perfil de Recebimento']}`, 12, leftY);
      leftY += 4;
    }
    if (obs['Separação']) {
      doc.text(`Separação: ${obs['Separação']}`, 12, leftY);
      leftY += 4;
    }
    if (obs['Perfil de paletização']) {
      doc.text(`Paletização: ${obs['Perfil de paletização']}`, 12, leftY);
      leftY += 4;
    }
    
    // Coluna direita
    let rightY = yPos;
    if (obs['Estabilização do palete']) {
      doc.text(`Estabilização: ${obs['Estabilização do palete']}`, 110, rightY);
      rightY += 4;
    }
    if (obs['Altura do palete']) {
      doc.text(`Altura do palete: ${obs['Altura do palete']}m`, 110, rightY);
      rightY += 4;
    }
    if (obs['Aceita Sobreposição']) {
      doc.text(`Aceita Sobreposição: ${obs['Aceita Sobreposição']}`, 110, rightY);
      rightY += 4;
    }
    
    yPos = Math.max(leftY, rightY) + 5;
  } else {
    // Se não houver observações, manter espaço vazio
    doc.setDrawColor(...borderColor);
    doc.rect(10, yPos, 190, 25, 'D');
    yPos += 30;
  }
  
  yPos += 5;

  // ===== TABELA DE PRODUTOS =====
  // Cabeçalho da tabela
  doc.setFillColor(...grayBg);
  doc.setDrawColor(...borderColor);
  doc.rect(10, yPos, 277, 8, 'FD');
  
  doc.setFontSize(9);
  doc.setFont('helvetica', 'bold');
  doc.text('SKU', 12, yPos + 5);
  doc.text('PRODUTO', 40, yPos + 5);
  doc.text('UMD', 180, yPos + 5);
  doc.text('QUANTIDADE', 200, yPos + 5);
  doc.text('STATUS', 230, yPos + 5);
  doc.text('OBS', 255, yPos + 5);
  
  yPos += 8;

  yPos += 8;

  // Linhas dos produtos
  doc.setFont('helvetica', 'normal');
  doc.setFontSize(8);
  
  let totalQuantidade = 0;
  
  if (data.produtos && data.produtos.length > 0) {
    // Múltiplos produtos
    data.produtos.forEach((produto, index) => {
      if (yPos > 270) {
        doc.addPage();
        yPos = 20;
      }

      const sku = produto.codigo || '-';
      const produtoTexto = produto.produto || '-';
      const unidade = produto.unidade || 'UN';
      const quantidade = produto.quantidade ? parseFloat(produto.quantidade).toFixed(0) : '0';
      
      totalQuantidade += parseFloat(produto.quantidade || 0);

      // Truncar produto se muito longo
      const produtoTruncado = produtoTexto.length > 80 ? produtoTexto.substring(0, 80) + '...' : produtoTexto;
      
      doc.text(sku, 12, yPos + 4);
      doc.text(produtoTruncado, 40, yPos + 4);
      doc.text(unidade, 180, yPos + 4);
      doc.text(quantidade, 210, yPos + 4);
      doc.text('', 230, yPos + 4); // Status vazio
      doc.text('', 255, yPos + 4); // OBS vazia
      
      doc.rect(10, yPos, 277, 7, 'D');
      yPos += 7;
    });
  } else if (data.produto) {
    // Produto único
    const sku = data.codigo || '-';
    const produto = data.produto || '-';
    const unidade = data.unidade || 'UN';
    const quantidade = data.quantidade ? parseFloat(data.quantidade).toFixed(0) : '0';
    
    totalQuantidade = parseFloat(data.quantidade || 0);
    
    const produtoTexto = produto.length > 80 ? produto.substring(0, 80) + '...' : produto;
    
    doc.text(sku, 12, yPos + 4);
    doc.text(produtoTexto, 40, yPos + 4);
    doc.text(unidade, 180, yPos + 4);
    doc.text(quantidade, 210, yPos + 4);
    doc.text('', 230, yPos + 4);
    doc.text('', 255, yPos + 4);
    
    doc.rect(10, yPos, 277, 7, 'D');
    yPos += 7;
  }

  // Linha Total Geral
  doc.setFillColor(240, 240, 240);
  doc.rect(10, yPos, 277, 7, 'FD');
  doc.setFont('helvetica', 'bold');
  doc.text('Total Geral', 12, yPos + 4);
  doc.text(totalQuantidade.toFixed(0), 210, yPos + 4);

  return doc;
};

/**
 * Download PDF file
 * @param {jsPDF} doc - The PDF document
 * @param {string} filename - The filename for download
 */
export const downloadPDF = (doc, filename = 'checklist-ctrc.pdf') => {
  doc.save(filename);
};

/**
 * Generate and download PDF from CTRC data
 * @param {Object} data - The CTRC data
 */
export const generateAndDownloadPDF = (data) => {
  const doc = generatePDF(data);
  const filename = `checklist-ctrc-${data.ctrc || 'sem-numero'}-${Date.now()}.pdf`;
  downloadPDF(doc, filename);
};
